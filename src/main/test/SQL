CREATE TABLE IF NOT EXISTS CLIENTE (
	ID_CLIENTE INT NOT NULL AUTO_INCREMENT,
    NOMBRE VARCHAR(100),
    PRIMARY KEY(ID_CLIENTE)
)ENGINE=INNODB;

